{
    "name": "cottontail-frontend",
    "version": "0.1.0",
    "license": "Apache-2.0",
    "homepage": "https://github.com/rabix/cottontail-frontend#readme",
    "angular-cli": {},
    "bugs": {
        "url": "https://github.com/rabix/cottontail-frontend/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rabix/cottontail-frontend.git"
    },
    "scripts": {
        "serve": "./node_modules/.bin/ng serve",
        "electron:compile": "./node_modules/.bin/tsc -p electron || cp -R electron/src/splash electron/dist/src",
        "electron:compile:watch": "./node_modules/.bin/tsc -p electron --watch",
        "electron": "./node_modules/.bin/electron ./electron",
        "postinstall": "npm run update-webdriver && cd electron && npm install",
        "build": "./node_modules/.bin/ng build -prod -aot",
        "package": "./scripts/electron-build.sh",
        "test": "./node_modules/.bin/ng test",
        "test-once": "./node_modules/.bin/ng test --watch false",
        "lint": "./node_modules/.bin/ng lint",
        "e2e": "./node_modules/.bin/ng e2e",
        "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/router": "^4.0.0",
        "ajv": "^4.11.3",
        "angular2-moment": "^1.2.0",
        "bootstrap": "4.0.0-alpha.5",
        "brace": "^0.10.0",
        "core-js": "^2.4.1",
        "cwl-svg": "^0.0.28",
        "cwlts": "^1.12.9",
        "font-awesome": "^4.7.0",
        "jquery": "^3.2.0",
        "js-yaml": "^3.8.2",
        "markdown-it-lazy-headers": "^0.1.3",
        "rxjs": "^5.1.0",
        "selectize": "^0.12.4",
        "snapsvg-cjs": "^0.0.4",
        "zone.js": "^0.8.5"
    },
    "devDependencies": {
        "@angular/cli": "^1.0.0",
        "@angular/compiler-cli": "^4.0.0",
        "@types/ace": "^0.0.32",
        "@types/jasmine": "2.5.38",
        "@types/jquery": "^2.0.41",
        "@types/js-yaml": "^3.5.29",
        "@types/markdown-it": "0.0.2",
        "@types/node": "~6.0.60",
        "@types/selectize": "^0.12.28",
        "@types/snapsvg": "^0.4.27",
        "codelyzer": "^2.0.0",
        "electron": "^1.6.2",
        "electron-packager": "^8.5.2",
        "glob": "^7.1.1",
        "jasmine-core": "~2.5.2",
        "jasmine-spec-reporter": "~3.2.0",
        "karma": "~1.4.1",
        "karma-chrome-launcher": "~2.0.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^0.2.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "markdown-it": "^8.3.0",
        "mocha": "^3.2.0",
        "protractor": "~5.1.0",
        "ts-node": "~2.0.0",
        "tslint": "^4.5.1",
        "typescript": "^2.2.1"
    },
    "engines": {
        "node": ">= 7.4.0",
        "npm": ">= 3"
    }
