{
    "name": "cottontail-frontend",
    "version": "0.1.0",
    "license": "Apache-2.0",
    "homepage": "https://github.com/rabix/cottontail-frontend#readme",
    "bugs": {
        "url": "https://github.com/rabix/cottontail-frontend/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rabix/cottontail-frontend.git"
    },
    "scripts": {
        "build": "npm run build:dev",
        "build:dev": "npm run webpack -- --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
        "build:electron": "npm run clean:dist && npm run webpack -- --config config/webpack.electron.prod.js --colors --display-error-details && npm --prefix ./electron install ./electron",
        "build:prod": "npm run webpack -- --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached",
        "ci": "npm run e2e && npm run test",
        "clean": "npm cache clean && npm run rimraf -- node_modules coverage dist",
        "clean:dist": "npm run rimraf -- dist",
        "clean:install": "npm set progress=false && npm install",
        "clean:start": "npm start",
        "debug:test": "npm run watch:test -- --debug",
        "lint": "npm run tslint 'src/**/*.ts'",
        "package": ". scripts/electron-build.sh",
        "prebuild:dev": "npm run clean:dist",
        "prebuild:prod": "npm run clean:dist",
        "preclean:install": "npm run clean",
        "preclean:start": "npm run clean",
        "rimraf": "node_modules/rimraf/bin.js",
        "server": "npm run server:dev",
        "deps:install": ". ./scripts/install-deps.sh",
        "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --inline --progress --profile --colors --watch --display-cached --content-base src/",
        "server:electron": "npm run webpack-dev-server -- --config config/webpack.electron.dev.js --inline --colors --watch --content-base src/",
        "start": "npm run server:dev",
        "start:electron": "npm run server:electron",
        "test": "npm run test:unit && npm run test:electron:frontend && npm run test:electron:backend",
        "test:unit": "node --max-old-space-size=4096 node_modules/karma/bin/karma start",
        "test:electron:frontend": "node_modules/.bin/mocha --ui tdd ./e2e/electron/**/*.js ",
        "test:electron:backend": "export TESTING=true; electron/node_modules/.bin/mocha --ui tdd ./electron/src/**/*.spec.js ",
        "tslint": "node_modules/tslint/bin/tslint",
        "watch": "npm run watch:dev",
        "watch:dev": "npm run build:dev -- --watch",
        "watch:prod": "npm run build:prod -- --watch",
        "watch:test": "npm run test -- --auto-watch --no-single-run",
        "webdriver:start": "npm run webdriver-manager start",
        "webdriver:update": "npm run webdriver-manager update",
        "webpack": "node_modules/webpack/bin/webpack.js",
        "webpack-dev-server": "node_modules/webpack-dev-server/bin/webpack-dev-server.js"
    },
    "dependencies": {
        "@angular/common": "^2.4.4",
        "@angular/compiler": "^2.4.4",
        "@angular/core": "^2.4.4",
        "@angular/forms": "^2.4.4",
        "@angular/http": "^2.4.4",
        "@angular/platform-browser": "^2.4.4",
        "@angular/platform-browser-dynamic": "^2.4.4",
        "ajv": "^4.8.2",
        "angular2-moment": "^1.1.0",
        "brace": "^0.9.0",
        "core-js": "^2.4.1",
        "cwlts": "^1.10.9",
        "font-awesome": "^4.6.1",
        "jailed": "^0.3.1",
        "jasmine-core": "^2.5.2",
        "js-yaml": "^3.6.1",
        "reflect-metadata": "^0.1.9",
        "rxjs": "~5.0.2",
        "zone.js": "0.7.4"
    },
    "devDependencies": {
        "@angular/compiler-cli": "~2.4.3",
        "@angular/language-service": "^2.4.1",
        "@types/ace": "0.0.31",
        "@types/core-js": "^0.9.35",
        "@types/electron": "^1.4.24",
        "@types/jasmine": "^2.2.34",
        "@types/js-yaml": "^3.5.29",
        "@types/node": "^7.0.0",
        "@types/selenium-webdriver": "~2.53.39",
        "@types/source-map": "^0.5.0",
        "@types/uglify-js": "^2.0.27",
        "@types/webpack": "^2.2.2",
        "add-asset-html-webpack-plugin": "^1.0.2",
        "angular2-template-loader": "^0.6.0",
        "assets-webpack-plugin": "^3.4.0",
        "awesome-typescript-loader": "~3.0.0-beta.18",
        "bootstrap": "4.0.0-alpha.5",
        "codelyzer": "~2.0.0-beta.4",
        "compression-webpack-plugin": "0.3.2",
        "copy-webpack-plugin": "^4.0.0",
        "css-loader": "^0.26.1",
        "electron": "1.4.14",
        "electron-packager": "^8.1.0",
        "exports-loader": "^0.6.3",
        "expose-loader": "^0.7.1",
        "extract-text-webpack-plugin": "^2.0.0-beta.5",
        "file-loader": "^0.9.0",
        "find-root": "^1.0.0",
        "html-webpack-plugin": "^2.26.0",
        "imports-loader": "^0.7.0",
        "istanbul-instrumenter-loader": "1.2.0",
        "jasmine-core": "^2.5.2",
        "json-loader": "^0.5.4",
        "karma": "^1.2.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-coverage": "^1.1.1",
        "karma-jasmine": "^1.0.2",
        "karma-mocha-reporter": "^2.0.0",
        "karma-remap-coverage": "^0.1.4",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "2.0.1",
        "mocha": "3.2.0",
        "ngc-webpack": "1.1.2",
        "node-sass": "^4.2.0",
        "npm-run-all": "^4.0.0",
        "parse5": "^3.0.1",
        "protractor": "^4.0.14",
        "raw-loader": "0.5.1",
        "rimraf": "~2.5.4",
        "sass-loader": "^4.1.1",
        "script-ext-html-webpack-plugin": "^1.3.2",
        "source-map-loader": "^0.1.5",
        "spectron": "3.4.1",
        "string-replace-loader": "1.0.5",
        "style-loader": "^0.13.1",
        "to-string-loader": "^1.1.4",
        "ts-helpers": "1.1.2",
        "ts-node": "^2.0.0",
        "tslint": "~4.3.1",
        "typedoc": "^0.5.3",
        "typescript": "~2.1.5",
        "url-loader": "^0.5.7",
        "v8-lazy-parse-webpack-plugin": "^0.3.0",
        "webpack": "^2.2.0",
        "webpack-dev-middleware": "^1.6.1",
        "webpack-dev-server": "2.2.0-rc.0",
        "webpack-dll-bundles-plugin": "^1.0.0-beta.2",
